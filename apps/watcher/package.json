{
  "name": "watcher",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "build": "esbuild --bundle src/index.ts --outdir=dist --minify --sourcemap --platform=node",
    "dev": "esbuild --bundle src/index.ts --outdir=dist --minify --sourcemap --platform=node --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "currency.js": "^2.0.4",
    "node-html-parser": "^5.3.3"
  },
  "devDependencies": {
    "config": "workspace:*",
    "esbuild": "^0.14.27",
    "eslint": "^8.11.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.6.2"
  }
}
